<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/editbasketballnews" method="post">
      <div class="form-group">
        <label>Season</label>
        <input class="form-control" type="text" name="basketballSeason" value="">
        <label>Title</label>
        <input class="form-control" type="text" name="basketballNewsTitle">
        <label>Post</label>
        <textarea class="form-control" name="basketballNewsBody" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>
